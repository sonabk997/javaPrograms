public class Tens {
    public static void main(String[] args) {
        int[] s = {10, 2, 3, 4, 10, 39, 10,10};
        boolean a = true;
        int sum = 0;
        for (int i = 0; i < s.length; i++) {
            if (s[i] ==10) {
                sum = sum + s[i];
            }
        }
        if (sum == 30){
            System.out.println(a);
        }else{
            a =  false;
            System.out.println(a);

    }}
}

class Main {
    public String name;
     public String getName(){
         return name;
     }
     public void setName(String newName){
         this.name = newName;
     }
    public static void main(String[] args) {
        Main s = new Main();
          s.setName("Sona");
       String g =  s.getName();
        System.out.println(g);
    }
}

